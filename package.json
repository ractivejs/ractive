{
  "name": "ractive",
  "version": "0.9.0-edge",
  "license": "MIT",
  "private": true,
  "engines" : {
    "node" : ">=4.0.0",
    "npm" : ">=2.14.2"
  },
  "devDependencies": {
    "cheerio": "^0.22.0",
    "coveralls": "~2.11",
    "eslint": "^3.7.1",
    "gobble": "^0.10.1",
    "gobble-buble": "^0.14.0",
    "gobble-cli": "^0.6.0",
    "gobble-replace": "^0.3.1",
    "gobble-rollup": "^0.36.0",
    "junk": "^2.0.0",
    "karma": "~1.3",
    "karma-coverage": "~1.1",
    "karma-phantomjs-launcher": "~1.0",
    "karma-qunit": "~1.2",
    "phantomjs-prebuilt": "~2.1",
    "promises-aplus-tests": "^2.1.0",
    "qunit-cli": "^0.2.0",
    "qunitjs": "~2.0",
    "rollup": "^0.36.0",
    "rollup-plugin-buble": "^0.14.0",
    "sander": "^0.5.1",
    "sorcery": "^0.10.0",
    "surge": "^0.18.0",
    "uglify-js": "^2.6.2"
  },
  "scripts": {
    "start": "gobble serve",
    "lint:src": "eslint src/ --ext .js",
    "lint:test": "eslint test/node-tests/ test/browser-tests/ --ext .js",
    "test": "sh ./scripts/test.sh",
    "promises-aplus-tests": "./scripts/promises-aplus-tests.js",
    "build": "sh ./scripts/build.sh",
    "fakebuild": "sh ./scripts/build.sh --fake",
    "release": "sh ./scripts/release.sh",
    "edge-release": "sh ./scripts/edge-release.sh",
    "coverage": "karma start",
    "coveralls": "npm run coverage && cat ./tmp/coverage/lcov.info | coveralls && rm -rf ./tmp/coverage",
    "perf": "cd perf; gobble"
  }
}
