{
  "name": "ractive",
  "version": "1.0.0-edge",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.14.2"
  },
  "typings": "typings/ractive.d.ts",
  "devDependencies": {
    "@evs-chris/buble": "^0.15.2",
    "cheerio": "^1.0.0-rc.2",
    "coveralls": "^3.0.0",
    "electron": "^1.7.12",
    "eslint": "^4.16.0",
    "fs-plus": "^3.0.2",
    "gobble": "^0.12.0",
    "gobble-buble": "^0.14.0",
    "gobble-cli": "^0.8.0",
    "gobble-uglifyjs": "^0.2.1",
    "istanbul": "^0.4.5",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.2.2",
    "karma-failed-reporter": "0.0.3",
    "karma-qunit": "^1.2.1",
    "karma-sauce-launcher": "^1.2.0",
    "magic-string": "^0.22.4",
    "puppeteer": "^1.0.0",
    "qunitjs": "^2.4.1",
    "rollup": "^0.55.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-istanbul": "^2",
    "prettier": "^1.11"
  },
  "scripts": {
    "start": "gobble --env=dev:browser serve",
    "dev:browser": "gobble --env=dev:browser serve",
    "dev:node": "",
    "dev:bin": "",
    "dev:perf": "cd perf; gobble",
    "lint:src": "eslint src",
    "lint:tests:browser": "eslint tests/browser",
    "lint:tests:node": "eslint tests/node",
    "lint:bin": "",
    "lint:perf": "",
    "lint:lib": "",
    "prettier": "prettier --use-tabs --single-quote --trailing-comma-es5 --write \"{src,tests/browser,tests/node}/**/*.js\"",
    "bundle:test": "gobble --env=bundle:test build .build --force",
    "bundle:release": "gobble --env=bundle:release build .build --force",
    "test:electron": "karma start karma/electron.conf.js",
    "test:chrome": "karma start karma/chrome.conf.js",
    "test:coverage": "istanbul report --root .build/coverage --dir .build/coverage/combined",
    "test:bin": "",
    "test:lib": "",
    "build:local": "sh ./scripts/build-local.sh",
    "build:local:fast": "sh ./scripts/build-local-fast.sh",
    "build:ci:pr": "sh ./scripts/build-ci-pr.sh",
    "build:ci:dev": "sh ./scripts/build-ci-dev.sh",
    "report:coveralls": "cat .build/coverage/combined/lcov.info | coveralls",
    "release:manual:all": "sh ./scripts/release-manual-all.sh",
    "release:manual:edge": "sh ./scripts/release-manual-edge.sh"
  }
}
