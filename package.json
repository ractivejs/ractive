{
  "name": "ractive",
  "version": "1.0.0-edge",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=5.0.0"
  },
  "devDependencies": {
    "fs-plus": "^3.0.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-qunit": "^1.2.1",
    "karma-tape-reporter": "^1.0.3",
    "lerna": "^2.4.0",
    "puppeteer": "^0.12.0",
    "qunit": "^2.4.1",
    "qunit-assert-html": "^1.1.0",
    "qunitjs": "^2.4.0",
    "rollup": "^0.50.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "simulant": "^0.2.2",
    "standard": "^10.0.3"
  },
  "scripts": {
    "bootstrap": "npm install && lerna bootstrap",
    "clean": "npm run clean:lerna && npm run clean:deps && npm run clean:dist && npm run clean:tmp",
    "clean:lerna": "lerna clean --yes",
    "clean:deps": "rm -r node_modules",
    "clean:dist": "find . -name dist -type d -exec rm -r {} +",
    "clean:tmp": "find . -name tmp -type d -exec rm -r {} +",
    "build": "lerna run build",
    "test": "lerna run test"
  }
}
