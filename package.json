{
  "name": "ractive",
  "version": "1.5.0-edge",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "typings": "typings/ractive.d.ts",
  "devDependencies": {
    "@evs-chris/buble": "^0.15.2",
    "cheerio": "^1.0.0-rc.12",
    "coveralls": "^3.0.4",
    "electron": "^18.3.7",
    "eslint": "^4.19.1",
    "eslint-plugin-prettier": "^2.7",
    "fs-plus": "^3.1.1",
    "gobble": "^0.12.0",
    "gobble-buble": "^0.14.0",
    "gobble-cli": "^0.8.0",
    "gobble-uglifyjs": "^0.2.1",
    "istanbul": "^0.4.5",
    "karma": "^5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^2",
    "karma-electron": "^5.2.2",
    "karma-failed-reporter": "0.0.3",
    "karma-qunit": "^1.2.1",
    "karma-sauce-launcher": "^1.2.0",
    "magic-string": "^0.22.4",
    "prettier": "1.15.3",
    "puppeteer": "^1.17.0",
    "qunitjs": "^2.4.1",
    "rollup": "^0.55.3",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-istanbul": "^2"
  },
  "scripts": {
    "start": "gobble --env=dev:browser serve",
    "dev:browser": "gobble --env=dev:browser serve",
    "dev:node": "",
    "dev:bin": "",
    "dev:perf": "cd perf; gobble",
    "lint:src": "eslint src",
    "lint:tests:browser": "eslint tests/browser",
    "lint:tests:node": "eslint tests/node",
    "lint:all": "eslint src tests/browser tests/node bin lib",
    "lint:fix:src": "eslint --fix src",
    "lint:fix:tests:browser": "eslint --fix tests/browser",
    "lint:fix:tests:node": "eslint --fix tests/node",
    "lint:fix:all": "eslint --fix src tests/browser tests/node bin lib",
    "lint:bin": "eslint bin lib",
    "lint:fix:bin": "eslint --fix bin lib",
    "lint:perf": "",
    "bundle:dev": "gobble --env=bundle:dev build .build -f",
    "bundle:test": "gobble --env=bundle:test build .build -f",
    "bundle:release": "gobble --env=bundle:release build .build -f",
    "test:electron": "karma start karma/electron.conf.js",
    "test:chrome": "karma start karma/chrome.conf.js",
    "test:coverage": "istanbul report --root .build/coverage --dir .build/coverage/combined",
    "test:bin": "",
    "build:local": "sh ./scripts/build-local.sh",
    "build:local:fast": "sh ./scripts/build-local-fast.sh",
    "build:ci:pr": "sh ./scripts/build-ci-pr.sh",
    "build:ci:dev": "sh ./scripts/build-ci-dev.sh",
    "report:coveralls": "cat .build/coverage/combined/lcov.info | coveralls",
    "release:manual": "sh ./scripts/release-manual.sh"
  }
}
