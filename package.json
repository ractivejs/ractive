{
  "name": "ractive",
  "version": "1.0.0-edge",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "typings": "typings/ractive.d.ts",
  "devDependencies": {
    "@reactivex/rxjs": "^5.5.6",
    "ampersand-collection": "^2.0.2",
    "ampersand-model": "^8.0.1",
    "backbone": "^1.3.3",
    "baconjs": "^2.0.0",
    "browserify": "^15.2.0",
    "commander": "^2.14.0",
    "coveralls": "^3.0.0",
    "fs-plus": "^3.0.2",
    "istanbul": "^0.4.5",
    "jquery": "^3.3.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-qunit": "^1.2.1",
    "karma-tape-reporter": "^1.0.3",
    "lite-server": "^2.3.0",
    "nyc": "^11.4.1",
    "puppeteer": "^1.0.0",
    "qunit": "^2.5.0",
    "qunit-assert-html": "^1.1.0",
    "qunitjs": "^2.4.1",
    "rollup": "^0.55.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-istanbul": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "serialize-javascript": "^1.4.0",
    "simulant": "^0.2.2",
    "standard": "^10.0.3",
    "tippex": "^3.0.0",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "wipe": "find ./packages/node_modules/@ractivejs/ -type d \\( -name dist -o -name tmp -o -name bin -o -name coverage \\) -exec rm -rf {} +",
    "exec": "cd ./packages/node_modules/@ractivejs/$npm_config_with && npm run $npm_config_do",
    "both": "npm run exec --do='make' --with=\"$npm_config_with\" && npm run exec --do='test' --with=\"$npm_config_with\"",
    "make": "< .build xargs -I % npm run exec --do='make' --with=\"%\"",
    "test": "< .build xargs -I % npm run exec --do='test' --with=\"%\"",
    "lcov": "istanbul report --root ./packages/node_modules/@ractivejs/ html lcov",
    "send": "< ./coverage/lcov.info coveralls",
    "srvr": "lite-server",
    "gone:nightly": "sh ./scripts/gone-nightly.sh"
  }
}
