{
  "name": "ractive",
  "version": "1.0.0-edge",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.14.2"
  },
  "devDependencies": {
    "buble": "^0.15.2",
    "cheerio": "^0.22.0",
    "coveralls": "^2.12.0",
    "electron": "^1.4.15",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-plus": "^3.0.0",
    "gobble": "^0.12.0",
    "gobble-cli": "^0.8.0",
    "gobble-uglifyjs": "^0.2.1",
    "istanbul": "^0.4.5",
    "karma": "^1.4.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-failed-reporter": "0.0.3",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-qunit": "^1.2.1",
    "karma-sauce-launcher": "^1.1.0",
    "magic-string": "^0.21.0",
    "phantomjs-prebuilt": "^2.1.14",
    "qunitjs": "^2.1.1",
    "rollup": "^0.41.0",
    "rollup-plugin-istanbul": "^1"
  },
  "scripts": {
    "start": "gobble --env=dev:browser serve",
    "dev:browser": "gobble --env=dev:browser serve",
    "dev:node": "",
    "dev:bin": "",
    "lint:src": "eslint src",
    "lint:tests:browser": "eslint tests/browser",
    "lint:tests:node": "eslint tests/node",
    "lint:bin": "",
    "lint:perf": "",
    "lint:lib": "",
    "bundle:test": "gobble --env=bundle:test build .build --force",
    "bundle:release": "gobble --env=bundle:release build .build --force",
    "test:electron": "cd .build; karma start ../karma/electron.conf.js",
    "test:phantom": "cd .build; karma start ../karma/phantom.conf.js",
    "test:saucelabs": "karma start karma/sauce.conf.js",
    "test:coverage": "cd .build; istanbul report --root coverage --dir coverage/combined",
    "test:bin": "",
    "test:lib": "",
    "build:local": "sh ./scripts/build-local.sh",
    "build:local:fast": "sh ./scripts/build-local-fast.sh",
    "build:ci:pr": "sh ./scripts/build-ci-pr.sh",
    "build:ci:dev": "sh ./scripts/build-ci-dev.sh",
    "report:coveralls": "cat .build/coverage/combined/lcov.info | coveralls",
    "release:manual:all": "sh ./scripts/release-manual-all.sh",
    "release:manual:edge": "sh ./scripts/release-manual-edge.sh"
  }
}
